{
  "manifest_version": 2,
  "name": "External Server",
  "short_name": "Server",
  "description": "",
  "version": "0.0.1",
  "minimum_chrome_version": "38",

  "icons": {
    "16": "../assets/cloud-flat.png",
    "32": "../assets/cloud-flat.png",
    "64": "../assets/cloud-flat.png",
    "128": "../assets/cloud-flat.png"
  },

  "background": {
    "scripts": ["background.js"]
  },
  
  "permissions": [
    "https://*/*",
    "http://*/*",
    "contextMenus",
    "tabs"
  ]
}
